<br> <br> <br> <br> <br> 
<div class="show_table">
	<h1>Schedule</h1>
<p>
  <b>File name:</b>
  <%= @schedule.file_name %>
</p>

<p>
  <b>Date uploaded:</b>
  <%= @schedule.date_uploaded %>
</p>

<p>
  <b>Schedule:</b>
  <%= link_to 'View', @schedule.schedule.url %>
</p>

<p>
  <b>Semester year:</b>
  <%= @schedule.semester_year %>
</p>

<p>
   <b>Registrar:</b> 
   <% temp = @schedule.registrar_id %>
  <%= Registrar.find(temp).proper_name %>
</p>

<p>
   <b>University:</b>
	<% temp2 = @schedule.registrar_id %>
	<% temp3 = Registrar.find(temp2).university_id %>
	<%= University.find(temp3).name %>
</p>

<% if can? :update, @schedule %>
<%= link_to 'Edit', edit_schedule_path(@schedule) %> |
<% end %>
<%= link_to 'Back', schedules_path %>
</div>
