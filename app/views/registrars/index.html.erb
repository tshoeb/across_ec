<table class="gridtable">
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Phone</th>
    <th colspan="3"> Actions </th>

  </tr>

<% @registrars.each do |registrar| %>
  <tr>
    <% if current_registrar != 0 || registrar.admin == false || current_registrar.admin == true %>
    <% if current_student  != 0 && registrar.admin == false %>
    <td><%= registrar.first_name %></td>
    <td><%= registrar.last_name %></td>
    <td><%= registrar.email %></td>
    <td><%= registrar.phone %></td>
    <td><%= link_to "Show", registrar %></td>
    <% if can? :update, @registrar %>
      <td><%= link_to "Edit", edit_registrar_path(registrar) %></td>
    <% end %>
    <% if can? :destroy, @registrar %>
    <td><%= button_to "delete", registrar, :method => :delete %></td>
    <% end %>
    <% end %>
    <% end %>
  </tr>
<% end %>
</table>

<br />
<% if can? :create, @registrar %>
<%= link_to 'New Registrar', new_registrar_path %>
<% end %>
