<p id="notice"><%= notice %></p>
<br> <br> <br> <br> <br> 
<p>
  <b>First name:</b>
  <%= @registrar.first_name %>
</p>

<p>
  <b>Last name:</b>
  <%= @registrar.last_name %>
</p>

<p>
  <b>University:</b>
  <% temp = @registrar.university_id %>
  <%= University.find(temp).name %>
</p>

<p>
  <b>Email:</b>
  <%= @registrar.email %>
</p>

<p>
  <b>Phone:</b>
  <%= @registrar.phone %>
</p>
<p>
  <b>Admin:</b>
  <%= @registrar.admin? %>
</p>

<% if can? :update, @registrar %>
	<%= link_to 'Edit', edit_registrar_path(@registrar) %> |
<% end %>
<%= link_to 'Back', registrars_path %>
