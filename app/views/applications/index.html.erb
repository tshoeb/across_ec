<h1>Listing applications</h1>

<table>
  <tr>
    <th>Application date</th>
    <th>Course no</th>
    <th>University</th>
    <th>Passport Copy</th>
    <th>Sign Form</th>
    <th>Semester year</th>
    <th>Registrar</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @applications.each do |application| %>
  <tr>
    <td><%= application.application_date %></td>
    <td><%= application.course_no %></td>
    <td><%= application.university_id %></td>
    <td><%= application.passport_copy %></td>
    <td><%= application.sign_form %></td>
    <td><%= application.semester_year %></td>
    <td><%= application.registrar_id %></td>
    <td><%= link_to 'Show', application %></td>
    <td><%= link_to 'Edit', edit_application_path(application) %></td>
    <td><%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Application', new_application_path %>
